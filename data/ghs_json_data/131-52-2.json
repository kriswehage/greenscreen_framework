    "systemic_toxicity_repeat_exposure": {
      "classification": [
        "Category 1 (skin, central nervous system, liver, kidneys); Category 2 (central nervous system)"
      ], 
      "hazard_id": [
        9.0
      ], 
      "hazard_name": [
        "systemic_toxicity_repeat_exposure"
      ], 
      "hazard_statement": [
        "Causes damage to organs (skin, central nervous system, liver, kidneys) through prolonged or repeated exposure; May cause damage to organs (central nervous system) through prolonged or repeated exposure"
      ], 
      "rationale": [
        "Since there is the statement of influence into the human skin (chloracne), liver, and kidney (EHC 71 (1987) of Priority 1 document), it was classified into Category 1 (skin, liver, kidney). Since it is supposed that it has also effect on a central nervous systems besides these organs (ICSC (J) (1997) of Priority 2 document), it was classified into Category 2 (central nervous systems)."
      ], 
      "signal_word": [
        "Danger"
      ], 
      "symbol": [
        "Health hazard"
      ]
    }, 
    "systemic_toxicity_single_exposure": {
      "classification": [

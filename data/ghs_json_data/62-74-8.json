    "systemic_toxicity_repeat_exposure": {
      "classification": [
        "Category 1 (heart, testes, nervous system, kidneys, liver)"
      ], 
      "hazard_id": [
        9.0
      ], 
      "hazard_name": [
        "systemic_toxicity_repeat_exposure"
      ], 
      "hazard_statement": [
        "Causes damage to organs (heart, testes, nervous system, kidneys, liver) through prolonged or repeated exposure"
      ], 
      "rationale": [
        "The effects on heart, testes, and central nervous system are admitted with the given dose of 0.5mg/kg/day by the examination for rats of EPA quoted by IRIS (1993). And in the example of poisoning in the people of HSDB (2005), since the effects on kidney, liver, and nervous system were reported, it was classified to as Category 1 (heart, teste, nervous system, kidney, and liver)."
      ], 
      "signal_word": [
        "Danger"
      ], 
      "symbol": [
        "Health hazard"
      ]
    }, 

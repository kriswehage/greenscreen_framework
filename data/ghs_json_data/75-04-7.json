    "systemic_toxicity_repeat_exposure": {
      "classification": [
        "Category 1 (respiratory organs); Category 2 (kidneys)"
      ], 
      "hazard_id": [
        9.0
      ], 
      "hazard_name": [
        "systemic_toxicity_repeat_exposure"
      ], 
      "hazard_statement": [
        "Causes damage to organs (respiratory organs) through prolonged or repeated exposure; May cause damage to organs (kidneys) through prolonged or repeated exposure"
      ], 
      "rationale": [
        "It was classified to as Category 1 (respiratory systems) based on the statement that peribronchial inflammation and interstitial pneumonias were observed in inhalation exposure of the guidance value range of Category 1of the rabbit (ACGIH (2001)). Moreover, it was classified to as Category 2 (kidney) based on the statement that the kidney parenchymatous degeneration was observed in inhalation exposure of the guidance value range of Category 2 of the rabbit (ACGIH (2001))."
      ], 
      "signal_word": [
        "Danger\r; Warning\r"
      ], 
      "symbol": [
        "Health hazard"
      ]

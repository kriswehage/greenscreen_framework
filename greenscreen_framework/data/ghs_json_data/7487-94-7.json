        "aspiration_hazard": {
            "classification": [
                "Category 1 (nervous system, kidney, respiratory system, cardiovascular system, gastrointestinal tract)",
                "Category 1 (central nervous system, kidney, thyroid, testis, cardiovascular system)"
            ],
            "hazard_id": [

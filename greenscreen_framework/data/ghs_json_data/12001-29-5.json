    "systemic_toxicity_repeat_exposure": {
      "classification": [
        "Category 1 (lung)"
      ],
      "hazard_id": [
        9.0
      ],
      "hazard_name": [
        "systemic_toxicity_repeat_exposure"
      ],
      "hazard_statement": [
        "Causes damage to organs (lung) through prolonged or repeated exposure"
      ],
      "rationale": [
        "Since pulmonary fibrosis was significantly observed in chrysotile laborers (EHC 203 (1998)), and pulmonary fibrosis was observed in rat study at 0.005 mg/L (equivalent) which is the ranges of the guidance value of Category 1 (EHC 203 (1998)), it was classified into Category 1 (lung)."
      ],
      "signal_word": [
        "Danger"
      ],
      "symbol": [
        "Health hazard"
      ]
    },

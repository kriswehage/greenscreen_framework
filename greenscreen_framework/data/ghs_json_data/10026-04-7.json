        "serious_eye_damage_irritation": {
            "classification": [
                "Category 1"
            ],
            "hazard_id": [
                3.0
            ],
            "hazard_name": [
                "serious_eye_damage_irritation"
            ],
            "hazard_statement": [
                "Causes serious eye damage"
            ],
            "rationale": [
                "The substance was found to cause severe burns on the cornea and lids in rabbit tests, and moderate irritation in Draize tests using rabbits that were treated with 20 mg for 24 hours (IUCLID (2000)). In addition, its classification for skin irritation and corrosion is Category 1. Based on this information, the substance was classified into Category 1."
            ],
            "signal_word": [
                "Danger"
            ],
            "symbol": [
                "Corrosion"
            ]
        },
        "skin_corrosion_irritation": {
            "classification": [
                "Category 1"
            ],
            "hazard_id": [
                2.0
            ],
            "hazard_name": [
                "skin_corrosion_irritation"
            ],
            "hazard_statement": [
                "Causes severe skin burns and eye damage"
            ],
            "rationale": [
                "The substance was found: to be corrosive in the tests using rabbits that were treated for 10 minutes (IUCLID (2000)); to be corrosive by an application of 0.5 ml for 10 minutes (IUCLID (2000)); to cause mild to moderate erythema and edema in Draize tests using rabbits that were treated for 3 minutes (IUCLID (2000)); to cause severe irritation in other Draize tests using rabbits that were treated with 500 mg for 24 hours (IUCLID (2000)). Based on these results, the substance was classified into Category 1."
            ],
            "signal_word": [
                "Danger"
            ],
            "symbol": [
                "Corrosion"
            ]
        },

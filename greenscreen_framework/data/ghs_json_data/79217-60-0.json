        "systemic_toxicity_repeat_exposure": {
            "classification": [
                "Category 1 (immune system)"
            ],
            "hazard_id": [
                8.0
            ],
            "hazard_name": [
                "systemic_toxicity_repeat_exposure"
            ],
            "hazard_statement": [
                "Causes damage to organs (immune system)"
            ],
            "rationale": [
                "Based on a report that the substance is a powerful immunosuppressive agent usually used in organ transplantation to prevent rejection (JAPIC prescription drug book (2010)), the substance was classified into Category 1 (immune system).\n"
            ],
            "signal_word": [
                "Danger"
            ],
            "symbol": [

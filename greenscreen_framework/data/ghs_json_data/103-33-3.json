            ],
            "hazard_name": [
                "aspiration_hazard"
            ],
            "hazard_statement": [
                "May cause damage to organs through prolonged or repeated exposure (liver)"
            ],
            "rationale": [
                "In oral administration test using dogs, the treated animals suffered from liver damage at the doses that fall under Category 2 guidance values (HSDB, 2003). Thus, the substance was classified into Category 2 (liver)."
            ],
            "signal_word": [
                "Warning"
            ],
            "symbol": [
                "Health Hazard"
            ]
        },
        "carcinogenicity": {
            "classification": [
                "Category 2"
            ],
            "hazard_id": [
                5.0
            ],
            "hazard_name": [
                "carcinogenicity"
            ],
            "hazard_statement": [
                "Suspected of causing genetic defects"
            ],
            "rationale": [
                "No test results in in-vivo germ cell mutagenicity studies are available, and an in-vitro micronucleus test using rat bone marrow cells and a mouse DNA damage test both gave positive results (RTECS (accessed in September 2007)). Based on these reports, the substance was classified into Category 2."
            ],
            "signal_word": [
                "Warning"
            ],
            "symbol": [
                "Health Hazard"
            ]
        },
        "chronic_aquatic_toxicity": {
            "classification": [
                "Category 1"
            ],
            "hazard_id": [
                11.0
            ],
            "hazard_name": [
                "chronic_aquatic_toxicity"
            ],
            "hazard_statement": [
                "Very toxic to aquatic life with long lasting effects"
            ],
            "rationale": [
                "Its classification for acute toxicity is Category 1, it is not rapidly degradable (BIOWIN 2007), and NOEC is < 1 mg/L (ECETOC TR 91 (2003)). Based on this information, the substance was classified into Category 1 for its chronic toxicity."
            ],

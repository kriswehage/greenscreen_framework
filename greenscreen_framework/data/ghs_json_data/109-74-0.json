        "systemic_toxicity_repeat_exposure": {
            "classification": [
                "Category 1 (systemic toxicity)"
            ],
            "hazard_id": [
                8.0
            ],
            "hazard_name": [
                "systemic_toxicity_repeat_exposure"
            ],
            "hazard_statement": [
                "Causes damage to organs (systemic toxicity)"
            ],
            "rationale": [
                "The symptoms were weakness, tremors, vasodilation, labored respiration and convulsions in a rat acute oral toxicity test (LD50 value: 50 - 100 mg/kg) (PATTY (5th, 2001)). Additionally, it was reported that with a single dose of 1440 mg/kg, rats became comatose and cyanotic and all animals died within 90 minutes (HSDB (2003)). From the LD50 values reported above, these toxic effects are considered to be observed at dose levels exceeding or within the guidance value range for Category 1 and the affected organ cannot be identified from the available data, therefore, the substance was classified into Category 1 (systemic toxicity)."
            ],
            "signal_word": [
                "Danger"
            ],
            "symbol": [
                "Health Hazard"
            ]
        },

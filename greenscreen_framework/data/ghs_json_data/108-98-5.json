        "systemic_toxicity_repeat_exposure": {
            "classification": [
                "Category 2 (nervous system),\nCategory 3 (respiratory tract irritation)"
            ],
            "hazard_id": [
                8.0
            ],
            "hazard_name": [
                "systemic_toxicity_repeat_exposure"
            ],
            "hazard_statement": [
                "May cause damage to organs (nervous system)\nMay cause respiratory irritation (respiratory tract irritation)"
            ],
            "rationale": [
                "Based on the human evidence including \"choking, irritation of the eye/nose, headache\" (MOE Risk Assessment vol.4 (2005)), and the evidence from animal studies including \"somnolency, coma, dyspnea\", \"irritation of the respiratory organs,\" \"ataxia,\" \"muscular weakness, ataxia, cyanosis\" (RTECS (2004)). The effects on experimental animals were observed at dosing levels within the guidance value ranges for Category 1. "
            ],
            "signal_word": [
                "Warning"
            ],
            "symbol": [
                "Health Hazard\nExclamation mark"
            ]
        },

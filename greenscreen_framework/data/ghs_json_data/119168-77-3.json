            "hazard_id": [
                7.0
            ],
            "hazard_name": [
                "systemic_toxicity_single_exposure"
            ],
            "hazard_statement": [
                "Suspected of damaging fertility or the unborn child"
            ],
            "rationale": [
                "Based on the evidence of abortion at doses inducing reduced body weight in parental animals in rabbit teratogenicity studies. Also based on the evidence of reduced fetal development and increased incidence of 14th rib anomalies at doses causing reduced body weight gains in parental animals in rat teratogenicity studies, reported in Agricultural Chemical Registration Data (1993).\n"
            ],
            "signal_word": [
                "Warning"
            ],

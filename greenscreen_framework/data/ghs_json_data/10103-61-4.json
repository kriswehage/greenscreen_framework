        "systemic_toxicity_repeat_exposure": {
            "classification": [
                "Category 1 (digestive system, circulatory system, nervous system, blood system, respiratory system, skin, kidney, liver)",
                "Category 1 (digestive system, cardiovascular system, nervous system, kidney, liver, respiratory system)"
            ],

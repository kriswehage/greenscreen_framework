    "systemic_toxicity_single_exposure": {
      "classification": [
        "Category 1 (nervous system)"
      ],
      "hazard_id": [
        8.0
      ],
      "hazard_name": [
        "systemic_toxicity_single_exposure"
      ],
      "hazard_statement": [
        "Cause damage to organs (nervous system)"
      ],
      "rationale": [
        "It was considered as Category 1(nerve systems) based on the description (Agricultural Chemical Registration Data ) that in mice and rats at the dose within the range of guidance value in Category 1 (more than 64mg/kg), entire body shivering, salivation, proneness position accompanied by dacryorrhea and a clonic convulsion, respiration depressed, protrusion of eyeballs, corneal reflex fall, and bloody tears were observed."
      ],
      "signal_word": [
        "Danger"
      ],
      "symbol": [
        "Health hazard"
      ]
    },

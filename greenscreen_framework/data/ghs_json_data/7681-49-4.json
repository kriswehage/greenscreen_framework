      "hazard_id": [
        7.0
      ], 
      "hazard_name": [
        "toxic_to_reproduction"
      ], 
      "hazard_statement": [
        ""
      ], 
      "rationale": [
        "Based on the evidence of histological changes in reproductive organs and reduced fertility of parental animals, along with the evidence of skeletal and visceral abnormalities in fetuses at maternally toxic doses, described in ATSDR (2003) and EHC 227 (2002)."
      ], 
      "signal_word": [
        ""
      ], 

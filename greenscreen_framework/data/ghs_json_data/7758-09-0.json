    "systemic_toxicity_single_exposure": {
      "classification": [
        "Category 2 (cardiovascular system, blood system); Category 3 (respiratory tract irritation)"
      ],
      "hazard_id": [
        8.0
      ],
      "hazard_name": [
        "systemic_toxicity_single_exposure"
      ],
      "hazard_statement": [
        "May cause damage to organs (cardiovascular system, blood system); May cause respiratory irritation or may cause drowsiness and dizziness (respiratory tract irritation)"
      ],
      "rationale": [
        "The substance was classified as Category 2 (cardio-vascular system, blood) and Category 3 (airway irritant) based on the reports in Priority 2 documents of the effects on the cardio-vascular system (ICSC, 2000; RTECS, 1997), and of the effects on blood (formation of methemoglobin) (ICSC, 2000; HSFS, 2004), and of airway irritant properties (ICSC, 2000; SITTIG, 2002; HSFS, 2004) in humans."
      ],
      "signal_word": [
        "Warning"
      ],
      "symbol": [
        "Health hazard\r; Exclamation mark\r"
      ]
    },

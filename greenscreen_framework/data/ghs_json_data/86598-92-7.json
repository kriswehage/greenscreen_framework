        "aspiration_hazard": {
            "classification": [
                "Category 2 (blood system, spleen, urinary bladder, liver)"
            ],
            "hazard_id": [
                9.0
            ],
            "hazard_name": [
                "aspiration_hazard"
            ],
            "hazard_statement": [
                "May cause damage to organs through prolonged or repeated exposure (blood system, spleen, urinary bladder, liver)"
            ],
            "rationale": [
                "Based on the evidence from animal studies including hemolytic anemia, increased spleen weights, increased hemosiderin deposition in the spleen, darkening/swelling of the spleen and hepatic centrilobular hypertrophy. Bladder effects (edema, congestion, and hypertrophy of mucosal epithelial cells) were also noted (Agricultural Chemical Registration Data (1993)). These effects were observed at dosing levels within the guidance value ranges for Category 2. \n"
            ],
            "signal_word": [
                "Warning"
            ],
            "symbol": [
                "Health Hazard"
            ]
        },

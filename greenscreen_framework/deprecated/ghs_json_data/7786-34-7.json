        "systemic_toxicity_repeat_exposure": {
            "classification": [
                "Category 1 (nervous system)"
            ],
            "hazard_id": [
                9.0
            ],
            "hazard_name": [
                "systemic_toxicity_repeat_exposure"
            ],
            "hazard_statement": [
                "Causes damage to organs (nervous system) through prolonged or repeated exposure"
            ],
            "rationale": [
                "We classified it into Category 1 (nerve systems) based on the description that the neurological symproms oweing to cholinesterase activities inhibition were observed with the dose of the guidance value range of Category 1in the feeding oral administration tests using the rat and dog (ACGIH (2003)), and based on the description that the effects on the nerve systems was observed in the 28-day oral study with a low-dose for the humans (ACGIH (2003))."
            ],
            "signal_word": [
                "Danger"
            ],
            "symbol": [
                "Health hazard"
            ]
        },
        "systemic_toxicity_single_exposure": {
            "classification": [
                "Category 1 (nervous system)"
            ],
            "hazard_id": [
                8.0
            ],
            "hazard_name": [
                "systemic_toxicity_single_exposure"
            ],
            "hazard_statement": [
                "Cause damage to organs (nervous system)"
            ],
            "rationale": [
                "The substance was classified as Category 1(nervous system) based on a report of neurological symptoms caused by the inhibition of cholinesterase activity in oral and percutaneous exposure cases in humans (ACGIH (2003))."
            ],
            "signal_word": [
                "Danger"
            ],
            "symbol": [
                "Health hazard"
            ]
        },

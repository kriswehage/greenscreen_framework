        "systemic_toxicity_single_exposure": {
            "classification": [
                "Category 1 (central nervous system); Category 3 (respiratory tract irritation)"
            ],
            "hazard_id": [
                8.0
            ],
            "hazard_name": [
                "systemic_toxicity_single_exposure"
            ],
            "hazard_statement": [
                "Cause damage to organs (central nervous system); May cause respiratory irritation or may cause drowsiness and dizziness (respiratory tract irritation)"
            ],
            "rationale": [
                "There is the effect to central nervous system (disappearance of righting reflex, incoordination, the abnormalities in a walk, weakness, headache, vomiting) in animal test (PATTY (5th, 2001), IUCLID (2000)), and there is the report that the symptoms such as headache, vomitting, incoordination, weakness even in human (IUCLID (2000), HSDB (2002)). So it is classified into Category 1 (central nervous). Moreover, there is the mild irritation to respiratory mucous membrane in both of human and animal (IUCLID (2000)). So it is classified into Category 3 (respiratory irritation)."
            ],
            "signal_word": [
                "Danger\r; Warning\r"
            ],
            "symbol": [

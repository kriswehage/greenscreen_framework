            "hazard_name": [
                "systemic_toxicity_repeat_exposure"
            ],
            "hazard_statement": [
                "May cause damage to organs (liver, thyroid gland) through prolonged or repeated exposure"
            ],
            "rationale": [
                "Because of the description that confirmation of increases in thyroid and liver weights, hepatocyte hypertrophy, decreased weight gain, and an increase of total cholesterol, with doses within the limits of the guidance value (10 - 100mg/kg) in Category 2 and higher in the repeated dose oral toxicity study with rats and mice (Agricultural Chemical Registration Data), it was classified into Category 2 (liver, thyroid gland)."
            ],
            "signal_word": [
                "Warning"
            ],
            "symbol": [
                "Health hazard"
            ]
        },

            ],
            "hazard_id": [
                8.0
            ],
            "hazard_name": [
                "systemic_toxicity_single_exposure"
            ],
            "hazard_statement": [
                "Cause damage to organs (nervous system)"
            ],
            "rationale": [
                "Since there is description (Agricultural Chemical Registration Data) that it is LD50=195mg/kg and emotions insecurity, tremor, the ataxia, spasm, and opisthotonos at the dosage near LD50=195mg/kg in a rat, were seen, it was thought that it was expressed within the range of the guidance value in Category 1 (below 300mg/kg), and was considered as Category 1 (nervous systems)."
            ],
            "signal_word": [
                "Danger"
            ],
            "symbol": [
                "Health hazard"
            ]
